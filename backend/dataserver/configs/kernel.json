{
  "display_name": "pyspark",
  "language": "python",
  "argv": [ "python3", "-m", "ipykernel", "-f", "{connection_file}" ],
  "env": {
    "SPARK_HOME": "/opt/spark",

    "PYTHONPATH": "/opt/spark/python/:/opt/spark/python/lib/py4j-0.10.7-src.zip",
    "PYSPARK_SUBMIT_ARGS": "--master k8s://kubernetes.default.svc:6443 --executor-memory 1G --total-executor-cores 1 pyspark-shell",
    "PYTHONSTARTUP": "/opt/spark/python/pyspark/shell.py"
  }
}

